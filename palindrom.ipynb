        "def palindrom(n):\n",
        "  s=0\n",
        "  temp=n\n",
        "  while(n>0):\n",
        "    r=n%10\n",
        "    s=s*10+r\n",
        "    n=n//10\n",
        "  print(\"Reverse is\",s)\n",
        "  if(temp==s):\n",
        "    print(\"Number is palindrom\")\n",
        "  else:\n",
        "    print(\"Number is not palindrom\")\n",
        "f=int(input(\"Enter the number: \"))\n",
        "palindrom(f)        "
      ]
